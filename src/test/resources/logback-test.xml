<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>[%-4level] [%logger{36}] [%d{HH:mm:ss.SSS}] %msg%n</pattern>
        </encoder>
    </appender>

    <root level="WARN">
        <!--<appender-ref ref="STDOUT" />-->
        <appender-ref ref="STDOUT" />
    </root>
</configuration>
